includes:
    - phpstan-baseline.neon

parameters:
    level: 5
    paths:
        - _custom
        - packages
        - app
    excludePaths:
        - 'packages/*/vendor/*'
        - 'packages/press/wordpress/*'
        - 'packages/press/src/Handlers'
        - 'packages/press/src/Transformer'
        - 'packages/press/src/Resolver'
        - 'packages/sync'
        - 'packages/*/tests/*'
    tmpDir: build/phpstan
