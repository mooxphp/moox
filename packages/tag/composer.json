{
    "name": "moox/tag",
    "description": "A simple Tag system for Filament.",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "dev-main",
        "moox/localization": "*",
        "moox/media": "*"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Tag\\": "src/",
            "Moox\\Tag\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Tag\\TagServiceProvider"
            ]
        },
        "moox": {
            "stability": "stable"
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "moox/devtools": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}