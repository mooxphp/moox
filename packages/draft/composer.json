{
    "name": "moox/draft",
    "description": "Draft is a simple Moox Entity, that can be used to create and manage simple entries, like logs.",
    "keywords": [
        "Moox",
        "Laravel",
        "Filament",
        "Moox package",
        "Laravel package"
    ],
    "homepage": "https://moox.org/docs/draft",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "*",
        "moox/localization": "*",
        "moox/media": "*",
        "moox/category": "*",
        "moox/tag": "*"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Draft\\": "src",
            "Moox\\Draft\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moox\\Draft\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Draft\\DraftServiceProvider"
            ]
        },
        "moox": {
            "name": "Moox Draft",
            "stability": "stable",
            "type": "moox-plugin",
            "install": {
                "auto_migrate": "database/migrations",
                "seed": "database/seeders/DraftSeeder.php",
                "auto_publish": "moox-draft-config",
                "auto_entities": {
                    "Draft": true
                },
                "auto_class": {
                    "Draft": "Moox\\Draft\\Draft"
                },
                "auto_run": {
                    "Run this": "php artisan run:this",
                    "Build the frontend": "npm run build",
                    "Clear the cache": "php artisan cache:clear"
                },
                "auto_runhere": {
                    "Build the frontend": "npm run build"
                }
            },
            "update": {
                "migrate": "database/migrations",
                "merge": "moox-draft-config"
            },
            "uninstall": {
                "migrate": "database/migrations",
                "remove": "moox-draft-config"
            }
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "moox/devtools": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}