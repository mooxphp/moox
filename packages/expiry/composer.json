{
    "name": "moox/expiry",
    "description": "Monitor the expiration of records",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "*"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Expiry\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Expiry\\ExpiryServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
