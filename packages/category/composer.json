{
    "name": "moox/category",
    "description": "This is my package category",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "codewithdennis/filament-select-tree": "4.x",
        "moox/core": "self.version",
        "moox/localization": "*",
        "kalnoy/nestedset": "^6.0",
        "moox/flag-icons-circle": "*",
        "moox/slug": "*"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Category\\": "src",
            "Moox\\Category\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Category\\CategoryServiceProvider"
            ]
        },
        "moox": {
            "name": "Moox Category",
            "stability": "stable",
            "type": "moox-plugin",
            "install": {
                "auto_migrate": "database/migrations",
                "seed": "database/seeders/CategorySeeder.php",
                "auto_publish": "moox-category-config",
                "auto_entities": {
                    "Category": true
                },
                "auto_class": {
                    "Category": "Moox\\Category\\Category"
                },
                "auto_run": {
                    "Run this": "php artisan run:this",
                    "Build the frontend": "npm run build",
                    "Clear the cache": "php artisan cache:clear"
                },
                "auto_runhere": {
                    "Build the frontend": "npm run build"
                }
            },
            "update": {
                "migrate": "database/migrations",
                "merge": "moox-category-config"
            },
            "uninstall": {
                "migrate": "database/migrations",
                "remove": "moox-category-config"
            }
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "moox/devtools": "*"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}