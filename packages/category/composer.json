{
    "name": "moox/category",
    "description": "This is my package category",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "codewithdennis/filament-select-tree": "4.x",
        "moox/core": "self.version",
        "moox/localization": "*",
        "kalnoy/nestedset": "^6.0",
        "moox/flag-icons-circle": "*",
        "moox/slug": "*"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Category\\": "src",
            "Moox\\Category\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Category\\CategoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "moox/devtools": "self.version"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}