{
    "name": "moox/laravel-icons",
    "description": "A package to easily make use of Icons in your Laravel Blade views.",
    "keywords": [
        "Blade",
        "Icons",
        "Laravel"
    ],
    "homepage": "https://github.com/mooxphp/laravel-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Devs"
        }
    ],
    "require": {
        "blade-ui-kit/blade-icons": "^1.6"
    },
    "require-dev": {
        "moox/devtools": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Moox\\LaravelIcons\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\LaravelIcons\\LaravelIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}