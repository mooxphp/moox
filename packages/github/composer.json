{
    "name": "moox/github",
    "description": "The Moox GitHub package. Which is a wrapper around the GitHub API implemented in Filament.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alf Drollinger",
            "email": "alf@moox.org"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-intl": "*",
        "composer-runtime-api": "^2.1",
        "symplify/monorepo-builder": "^12.0",
        "laravel/socialite": "^5.21"
    },
    "require-dev": {
        "moox/devtools": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Github\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Github\\GithubServiceProvider"
            ]
        },
        "moox-stability": "stable"
    },
    "config": {
        "sort-packages": true
    }
}