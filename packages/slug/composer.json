{
    "name": "moox/slug",
    "description": "This is my package slug",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Slug\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Slug\\SlugServiceProvider"
            ]
        },
        "moox": {
            "name": "Moox Slug",
            "stability": "stable",
            "type": "moox-plugin",
            "install": {
                "auto_migrate": "database/migrations",
                "seed": "database/seeders/SlugSeeder.php",
                "auto_publish": "moox-slug-config",
                "auto_entities": {
                    "Category": true
                },
                "auto_class": {
                    "Category": "Moox\\Slug\\Slug"
                },
                "auto_run": {
                    "Run this": "php artisan run:this",
                    "Build the frontend": "npm run build",
                    "Clear the cache": "php artisan cache:clear"
                },
                "auto_runhere": {
                    "Build the frontend": "npm run build"
                }
            },
            "update": {
                "migrate": "database/migrations",
                "merge": "moox-slug-config"
            },
            "uninstall": {
                "migrate": "database/migrations",
                "remove": "moox-slug-config"
            }
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "moox/devtools": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}