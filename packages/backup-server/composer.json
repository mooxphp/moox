{
    "name": "moox/backup-server",
    "description": "Filament UI for Spatie Laravel Backup Server.",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "dev-main",
        "spatie/laravel-backup-server": "^4.1"
    },
    "require-dev": {
        "moox/devtools": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Moox\\BackupServerUi\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\BackupServerUi\\BackupServerUiServiceProvider"
            ]
        },
        "moox": {
            "stability": "stable"
        }
    },
    "prefer-stable": true
}