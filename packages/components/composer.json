{
    "name": "moox/components",
    "description": "This is my package components",
    "keywords": [
        "Moox",
        "Laravel",
        "Filament",
        "Moox package",
        "Laravel package"
    ],
    "homepage": "https://moox.org/docs/components",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Components\\": "src"
        }
    },
    "extra": {
        "moox": {
            "title": "Moox Components",
            "released": false,
            "stability": "dev",
            "category": "development",
            "used_for": [
                "we do not know yet"
            ],
            "template_for": [
                "we do not know yet"
            ],
            "template_replace": {
                "Moox Developer": "%%Author%%",
                "dev@moox.org": "%%AuthorEmail%%",
                "components.jpg": "made-with-moox.jpg",
                "Components": "%%PackageName%%",
                "components": "%%PackageSlug%%",
                "This is my package components": "%%Description%%",
                "we do not know yet": "%%UsedFor%%",
                "we do not know yet": "%%TemplateFor%%"
            },
            "template_remove": [
                "build.php"
            ],
            "alternate_packages": [
                "builder"
            ]
        },
        "laravel": {
            "providers": [
                "Moox\\Components\\ComponentsServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "moox/devtools": "self.version"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
