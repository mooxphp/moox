{
    "name": "moox/user-device",
    "description": "This is my package user-device",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "geoip2/geoip2": "^3.0",
        "jenssegers/agent": "^2.6",
        "moox/core": "self.version"
    },
    "require-dev": {
        "moox/devtools": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Moox\\UserDevice\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\UserDevice\\UserDeviceServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
