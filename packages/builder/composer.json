{
    "name": "moox/builder",
    "description": "This template is used for generating all Moox packages.",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "spatie/laravel-package-tools": "^1.13.0",
        "filament/filament": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Builder\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Builder\\BuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
