<?php

namespace {{ namespace }};

use Filament\Panel;
use Filament\PanelProvider;
use {{ plugin_namespace }}\{{ class_name }}Plugin;

class {{ class_name }}PanelProvider extends PanelProvider
{
    public function panel(Panel $panel): Panel
    {
        return $panel
            ->id('{{ id }}')
            ->path('{{ path }}')
            ->plugin(new {{ class_name }}Plugin())
            ->login();
    }
}
