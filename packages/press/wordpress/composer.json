{
    "name": "moox/wp-install",
    "type": "wordpress-core",
    "autoload": {
        "psr-4": {
            "Moox\\WpInstall\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alf Drollinger",
            "email": "alf@drollinger.info"
        }
    ],
    "require": {
        "roots/wordpress": "^6.8",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "moox/devtools": "self.version"
    },
    "extra": {
        "wordpress-install-dir": "wp"
    },
    "config": {
        "allow-plugins": {
            "roots/wordpress-core-installer": true
        }
    }
}