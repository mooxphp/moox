{
    "name": "moox/press",
    "description": "This is my package press",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "dev-main",
        "adrolli/eloquent-mutators": "*",
        "moox/press": "dev-main"
    },
    "require-dev": {
        "moox/devtools": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Press\\": "src",
            "Moox\\Press\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Press\\PressServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}