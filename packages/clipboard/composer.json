{
    "name": "moox/clipboard",
    "description": "Filament fields with copy to clipboard action.",
    "type": "moox-plugin",
    "keywords": [
        "Moox",
        "Laravel",
        "Filament",
        "Moox package",
        "Laravel package"
    ],
    "homepage": "https://moox.org/docs/clipboard",
    "support": {
        "issues": "https://github.com/mooxphp/moox/issues",
        "source": "https://github.com/mooxphp/clipboard"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Moox",
            "email": "dev@moox.org",
            "role": "developer"
        }
    ],
    "require": {
        "filament/forms": "^4.0"
    },
    "require-dev": {
        "moox/devtools": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Clipboard\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Clipboard\\ClipboardServiceProvider"
            ]
        },
        "moox": {
            "stability": "stable"
        }
    }
}