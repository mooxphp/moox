{
    "name": "moox/skeleton",
    "description": "This template is used for generating Laravel packages, all Moox packages are built with this template.",
    "keywords": [
        "Moox",
        "Laravel",
        "Filament",
        "Moox package",
        "Laravel package"
    ],
    "homepage": "https://moox.org/docs/skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Skeleton\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Skeleton\\SkeletonServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "moox/devtools": "self.version"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
