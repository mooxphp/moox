{
    "name": "moox/monorepo",
    "description": "The Moox Monorepo containing Laravel and Filament packages (FOSS).",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alf Drollinger",
            "email": "alf@moox.org"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-intl": "*",
        "composer-runtime-api": "^2.1",
        "symplify/monorepo-builder": "^11.0",
        "laravel/socialite": "^5.21",
        "moox/core": "*"
    },
    "require-dev": {
        "moox/devlink": "*"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Monorepo\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Monorepo\\MonorepoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}