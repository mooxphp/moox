{
    "name": "moox/media",
    "description": "This is my package media",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "self.version",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "moox/localization": "*",
        "moox/data": "*"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Media\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Media\\MediaServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "moox/devtools": "self.version"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
