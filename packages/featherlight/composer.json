{
    "name": "moox/featherlight",
    "description": "This is my package featherlight",
    "keywords": [
        "Moox",
        "Laravel",
        "Filament",
        "Moox package",
        "Laravel package"
    ],
    "homepage": "https://moox.org/docs/featherlight",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Featherlight\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Featherlight\\FeatherlightServiceProvider"
            ]
        },
        "moox": {
            "stability": "stable"
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "moox/devtools": "self.version"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}