{
    "name": "moox/packages",
    "description": "Packages is a simple Moox Entity, that can be used to create and manage simple entries, like logs.",
    "keywords": [
        "Moox",
        "Laravel",
        "Filament",
        "Moox package",
        "Laravel package"
    ],
    "homepage": "https://moox.org/docs/packages",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Packages\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Packages\\PackagesServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "moox/devtools": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}