{
    "name": "moox/flag-icons-circle",
    "description": "Circle flags for all countries as Blade Components.",
    "keywords": [
        "Blade",
        "Flag Icons Circle",
        "Laravel"
    ],
    "homepage": "https://github.com/mooxphp/flag-icons-circle",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Devs",
            "email": "devs@moox.org"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Moox\\FlagIconsCircle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\FlagIconsCircle\\FlagIconsCircleServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}