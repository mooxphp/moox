{
    "name": "moox/press",
    "description": "This is my package press",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "spatie/laravel-package-tools": "^1.13.0",
        "johnpbloch/wordpress": "^6.3",
        "filament/filament": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Press\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Press\\PressServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}